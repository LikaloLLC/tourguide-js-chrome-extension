{
  "manifest_version": 3,
  "name": "Docsie Tour Guide",
  "version": "1.0.5",
  "icons": {
    "16": "images/icon-16.png",
    "24": "images/icon-24.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "24": "images/icon-24.png",
      "32": "images/icon-32.png"
    },
    "default_title": "Docsie Product Tours - Tour Guide"
  },
  "description": "Easily build, publish, update and maintain product tours in Docsie",
  "background": {
    "service_worker": "scripts/background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://app.docsie.io/*", "https://beta.docsie.io/*", "https://staging.docsie.io/*"],
      "js": ["scripts/docsie.js"]
    }
  ],
  "externally_connectable": {
    "ids": ["namnffimcbmhcpkllhaookfikmllhned"],
    "matches": ["https://app.docsie.io/*", "https://beta.docsie.io/*", "https://staging.docsie.io/*"]
  },
  "permissions": ["activeTab", "scripting", "webNavigation", "storage"],
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [
    {
      "resources": ["pages/*/*.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
